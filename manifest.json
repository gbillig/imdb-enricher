{
	"manifest_version": 2,

	"name": "IMDb Enricher",
	"description": "Adds the IMDb rating of each title in the filmography section of each person.",
	"version": "0.2",
	"permissions": [
	 "storage", "*://imdb.com/*", "http://www.omdbapi.com/*"
	],
	"icons": {
		"32": "icons/icon_32.png", 
		"48": "icons/icon_48.png", 
		"128": "icons/icon_128.png", 
		"512": "icons/icon_512.png"
	},
	"background": {
		"scripts": ["jquery-2.1.4.min.js"]
	},
	"content_scripts": [ {
		"js": ["jquery-2.1.4.min.js", "filmography.js" ],
		"run_at": "document_end",
		"matches": [ "http://www.imdb.com/name/*"]
	}]
}
